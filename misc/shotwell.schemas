            <key>/schemas/apps/shotwell/sharing/flickr/visibility</key>
            <applyto>/apps/shotwell/sharing/flickr/visibility</applyto>
            <type>int</type>
            <owner>shotwell</owner>
            <default>1</default>

            <locale name="en">
                <short>default visibility</short>
                <long>A numeric code representing the default visibility for photos published to Flickr</long>
            </locale>
