        <schema>
            <key>/schemas/apps/shotwell/sharing/picasa/auth_token</key>
            <applyto>/apps/shotwell/sharing/picasa/auth_token</applyto>
            <type>string</type>
            <owner>shotwell</owner>
            <default>NULL</default>

            <locale name="en">
                <short>auth token</short>
                <long>The authorization token for the currently logged in Picasa Web Albums user, if any</long>
            </locale>
        </schema>

        <schema>
            <key>/schemas/apps/shotwell/sharing/picasa/user_name</key>
            <applyto>/apps/shotwell/sharing/picasa/user_name</applyto>
